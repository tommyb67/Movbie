<div class="well">
  <h1>Movie Results</h1>
  <div class="row-fluid">
    <% @movie_list.each do |movie| %>
        <div class="span3 movie_list">
          <div class="movie_title">
            <h5><%= link_to movie.title, movie.id %></h5>
          </div>
        </div>
    <% end %>
  </div>
</div>